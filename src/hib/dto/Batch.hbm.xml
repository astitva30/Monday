<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    <class name="Batch" table="batchMonday">
        <id name="batchId" column="batchId"/>
        <property name="time" column="time"/>
        <property name="courseName" column="courseName"/>   
        <property name="strength" column="strength"/>
        <property name="coachName" column="coachName"/>      
    </class>
</hibernate-mapping>